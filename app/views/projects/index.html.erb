<h1>Projects</h1>
<%= link_to 'New Project', new_project_path, class: "btn btn-default" %>
<%= link_to 'Back', projects_path, class: "btn btn-default" %>
<% @projects.each do |project| %>
  <div class="row">
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <%= link_to project_path(project) do %> <img src="https://fbcdn-sphotos-d-a.akamaihd.net/hphotos-ak-xpa1/t31.0-8/1617411_10152327850315047_7165561517057286879_o.jpg" alt="holder.js/300x300">
        <div class="caption"><% end %>
          <h3><%= project.title %></h3>
          <p><%= project.description %></p>
          <p><%= link_to 'Edit', edit_project_path(project), class: "btn btn-primary"  %><%= link_to 'Delete', project, method: :delete, class: "btn btn-default" %></p>
        </div>
      </div>
    </div>
  </div>
<%end %>


